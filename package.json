{
  "name": "profanity",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy --minify",
    "deploy:dry-run": "wrangler deploy --dry-run --outdir=dist",
    "build": "wrangler deploy --dry-run --outdir=dist",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "seed": "tsx src/scripts/seed/index.ts",
    "upload": "tsx src/scripts/upload/index.ts",
    "seed:upload": "tsx src/scripts/seed/index.ts && tsx src/scripts/upload/index.ts"
  },
  "dependencies": {
    "hono": "^4.10.7"
  },
  "devDependencies": {
    "wrangler": "^4.4.0",
    "@types/node": "^22.10.2",
    "tsx": "^4.19.2"
  }
}